# Test to verify bytecode ordering
func make_counter() {
    print("Before let")
    let count := 0
    print("After let, before return")
    print(count)
    return func() {
        print("Inside inner func")
        count := count + 1
        return count
    }
}

print("Calling make_counter")
counter := make_counter()
print("Got counter, calling it")
print(counter())
