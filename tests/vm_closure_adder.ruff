# Test closure that just reads captured variable
func make_adder(x) {
    return func(y) {
        return x + y
    }
}

add5 := make_adder(5)
print(add5(3))   # Should print 8
print(add5(10))  # Should print 15

add10 := make_adder(10)
print(add10(5))  # Should print 15
print(add10(20)) # Should print 30
