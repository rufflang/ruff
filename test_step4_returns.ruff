# Test return values more carefully

func add(a, b) {
    return a + b
}

# Direct calls (not JIT'd)
print("5 + 3 =", add(5, 3))
print("10 + 20 =", add(10, 20))

# Now call 150 times to trigger JIT
for i in range(150) {
    x := add(i, 100)
    # Just call it, don't print
}

# Now test JIT-compiled version
print("After JIT compilation:")
print("5 + 3 =", add(5, 3))
print("10 + 20 =", add(10, 20))
print("100 + 200 =", add(100, 200))
