// Test file for JIT Call opcode support  
// This tests that functions with Call opcodes can be compiled

func add(a, b) {
    return a + b
}

func test() {
    return add(2, 3)
}

print("Starting test...")

// Call test() many times to trigger JIT compilation
result := 0
count := 0
for i in range(150) {
    result = test()
    count = count + 1
}

print("Count:", count)
print("Result:", result)
print("Test complete!")
