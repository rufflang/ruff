# Recursive Fibonacci Test

print("=== Testing Recursive Fibonacci ===")

func fib(n) {
    if n <= 1 {
        return n
    }
    return fib(n - 1) + fib(n - 2)
}

print("Test 1: Basic values")
print("fib(0) =", fib(0), "(expected 0)")
print("fib(1) =", fib(1), "(expected 1)")
print("fib(2) =", fib(2), "(expected 1)")
print("fib(3) =", fib(3), "(expected 2)")
print("fib(5) =", fib(5), "(expected 5)")
print("fib(10) =", fib(10), "(expected 55)")

print("Test 2: Medium values")
print("fib(15) =", fib(15), "(expected 610)")
print("fib(20) =", fib(20), "(expected 6765)")

print("Test 3: THE BIG TEST - fib(30)")
print("Python baseline: ~282ms")
print("Target: <50ms (5-10x faster)")
result := fib(30)
print("fib(30) =", result, "(expected 832040)")

print("=== Tests Complete ===")
