# Test HashMap operations

print("Testing HashMap with integer keys...")

func hash_map_ops(n) {
    map := {}
    i := 0
    while i < n {
        map[i] := i * 2
        i := i + 1
    }
    sum := 0
    i := 0
    while i < n {
        sum := sum + map[i]
        i := i + 1
    }
    return sum
}

print("Test 1: Small map (10 items)")
result1 := hash_map_ops(10)
print("Result:", result1, "(expected 90)")

print("Test 2: Medium map (100 items)")
result2 := hash_map_ops(100)
print("Result:", result2, "(expected 9900)")

print("Test 3: Large map (1000 items)")
result3 := hash_map_ops(1000)
print("Result:", result3, "(expected 999000)")

print("All tests complete!")
