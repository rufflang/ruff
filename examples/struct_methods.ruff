# Struct with methods

struct Rectangle {
    width: float,
    height: float,
    
    func area() {
        return width * height;
    }
    
    func perimeter() {
        return 2.0 * (width + height);
    }
}

# Create a rectangle
rect := Rectangle { width: 10.0, height: 5.0 };
print(rect);
print(rect.width);
print(rect.height);

# Call methods
area_result := rect.area();
print(area_result);

perim_result := rect.perimeter();
print(perim_result);

print("Struct methods complete");
