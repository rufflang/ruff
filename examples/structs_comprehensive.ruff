struct Rectangle {
    width: float,
    height: float,
    
    func area() {
        return width * height;
    }
    
    func perimeter() {
        return 2.0 * (width + height);
    }
}

rect := Rectangle { width: 10.0, height: 5.0 };
print(rect);
area_val := rect.area();
print(area_val);
perim_val := rect.perimeter();
print(perim_val);

print("Structs work!");
