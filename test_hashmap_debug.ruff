# Debug HashMap operations step by step

print("Step 1: Create empty map")
map := {}
print("Map created:", map)

print("Step 2: Set map[0] = 0")
map[0] := 0
print("Map after insert:", map)

print("Step 3: Set map[1] = 2")
map[1] := 2
print("Map after insert:", map)

print("Step 4: Read map[0]")
val0 := map[0]
print("map[0] =", val0)

print("Step 5: Read map[1]")
val1 := map[1]
print("map[1] =", val1)

print("Step 6: Try to add values")
sum := 0
print("Initial sum:", sum)
print("Adding map[0]...")
sum := sum + map[0]
print("Sum after adding map[0]:", sum)
print("Adding map[1]...")
sum := sum + map[1]
print("Sum after adding map[1]:", sum)

print("All steps complete!")
