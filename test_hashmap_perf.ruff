# HashMap Performance Test

func hash_map_ops(n) {
    map := {}
    i := 0
    while i < n {
        map[i] := i * 2
        i := i + 1
    }
    sum := 0
    i := 0
    while i < n {
        sum := sum + map[i]
        i := i + 1
    }
    return sum
}

print("HashMap Performance Test")
print("")

print("Test 1: 1k items")
start := performance_now()
result := hash_map_ops(1000)
elapsed := performance_now() - start
print("Result:", result, "Time:", elapsed, "ms")

print("Test 2: 10k items")
start := performance_now()
result := hash_map_ops(10000)
elapsed := performance_now() - start
print("Result:", result, "Time:", elapsed, "ms")

print("Test 3: 50k items")
start := performance_now()
result := hash_map_ops(50000)
elapsed := performance_now() - start
print("Result:", result, "Time:", elapsed, "ms")

print("Done!")
