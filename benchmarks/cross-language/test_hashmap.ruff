# Test hash map operations
func hash_map_ops(n) {
    map := {}
    i := 0
    while i < n {
        map[i] := i * 2
        i := i + 1
    }
    sum := 0
    i := 0
    while i < n {
        sum := sum + map[i]
        i := i + 1
    }
    return sum
}

# Small test first
print("Testing with n=10...")
result := hash_map_ops(10)
print("Result: ")
print(result)

# Medium test
print("Testing with n=1000...")
start := performance_now()
result := hash_map_ops(1000)
elapsed := performance_now() - start
print("Result: ")
print(result)
print("Time: ")
print(elapsed)
print("ms")
