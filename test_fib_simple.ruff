# Iterative Fibonacci Test

print("=== Testing Iterative Fibonacci ===")

func fib_iter(n) {
    if n <= 0 {
        return 0
    }
    if n == 1 {
        return 1
    }
    
    a := 0
    b := 1
    
    for i in range(n - 1) {
        temp := a
        a := b
        b := temp + b
    }
    
    return b
}

print("Test 1: Basic values")
print("fib_iter(0) =", fib_iter(0), "(expected 0)")
print("fib_iter(1) =", fib_iter(1), "(expected 1)")
print("fib_iter(2) =", fib_iter(2), "(expected 1)")
print("fib_iter(5) =", fib_iter(5), "(expected 5)")
print("fib_iter(10) =", fib_iter(10), "(expected 55)")

print("Test 2: Larger values")
print("fib_iter(15) =", fib_iter(15), "(expected 610)")
print("fib_iter(20) =", fib_iter(20), "(expected 6765)")

print("Test 3: Trigger JIT with 200 calls")
sum := 0
for i in range(200) {
    sum := sum + fib_iter(10)
}
print("Sum of 200 calls:", sum, "(expected 11000)")

print("=== Tests Complete ===")
