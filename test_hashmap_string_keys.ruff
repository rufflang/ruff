# Test HashMap with string keys

print("Testing HashMap with STRING keys...")

print("Step 1: Create and populate map")
map := {}
map["0"] := 0
map["1"] := 2
map["2"] := 4
print("Map:", map)

print("Step 2: Read values")
print("map[\"0\"] =", map["0"])
print("map[\"1\"] =", map["1"])
print("map[\"2\"] =", map["2"])

print("Step 3: Sum values")
sum := 0
sum := sum + map["0"]
print("After adding map[\"0\"]:", sum)
sum := sum + map["1"]
print("After adding map[\"1\"]:", sum)
sum := sum + map["2"]
print("After adding map[\"2\"]:", sum)

print("Final sum:", sum, "(expected 6)")
print("Test complete!")
